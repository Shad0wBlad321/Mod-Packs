<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<ThingDef ParentName="BenchBase">
		<defName>VFE_TableSmithyLargeGas</defName>
		<label>large helixien gas smithy</label>
		<description>A gas-powered work center for smithing non-mechanical weapons and tools. Consumes a lot of gas.</description>
		<thingClass>PipeSystem.Building_ResourceWorkTable_HeatPush</thingClass>
		<graphicData>
			<texPath>Things/Building/TableSmithyLargeGas</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(7,4)</drawSize>
			<shadowData>
				<volume>(2.8, 0.35, 0.59)</volume>
				<offset>(0, 0, 0.08)</offset>
			</shadowData>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<constructEffect>ConstructWood</constructEffect>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<hasInteractionCell>true</hasInteractionCell>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<designationCategory>VCHE_PipeNetworks</designationCategory>
		<surfaceType>Item</surfaceType>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
		<minifiedDef />
		<thingCategories Inherit="false" />
		<size>(5,2)</size>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>5000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Cleanliness>-3</Cleanliness>
			<WorkTableWorkSpeedFactor>1.5</WorkTableWorkSpeedFactor>
		</statBases>
		<costList>
			<Steel>200</Steel>
			<ComponentIndustrial>6</ComponentIndustrial>
		</costList>
		<recipes />
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
					<li>VFE_SmithyCabinet</li>
				</linkableFacilities>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>Things/Special/Fluid/VHGE_OverlayBase</commandTexture>
				<commandLabelKey>VHGE_CommandToggleGas_Label</commandLabelKey>
				<commandDescKey>VHGE_CommandToggleGas_Desc</commandDescKey>
			</li>
			<li Class="PipeSystem.CompProperties_ResourceTrader">
				<pipeNet>VHGE_HelixienNet</pipeNet>
				<consumptionPerTick>2</consumptionPerTick>
				<idleConsumptionPerTick>0</idleConsumptionPerTick>
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_HeatPusher">
				<compClass>PipeSystem.CompHeatPusherNeedResource</compClass>
				<heatPerSecond>4</heatPerSecond>
			</li>
		</comps>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<designationHotKey>Misc7</designationHotKey>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>Smithing</li>
			<li>VFE_Manufacturing</li>
			<li>VHGE_GasExtraction</li>
		</researchPrerequisites>
		<modExtensions>
			<li Class="RecipeInheritance.ThingDefExtension">
				<inheritRecipesFrom>
					<li>ElectricSmithy</li>
				</inheritRecipesFrom>
			</li>
		</modExtensions>
	</ThingDef>

