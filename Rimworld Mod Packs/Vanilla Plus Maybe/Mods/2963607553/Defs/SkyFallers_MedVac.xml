<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="ShuttleSkyfallerBase" Name="BDMEtest_ShuttleIncomingBase" Abstract="true">
		<thingClass>BillDoorsFramework.Skyfaller_Custom</thingClass>
		<rotatable>false</rotatable>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutFlying</shaderType>
			<color>(1,1,1)</color>
		</graphicData>
		<skyfaller>
			<anticipationSound>BDM_ThrusterApproaching</anticipationSound>
			<anticipationSoundTicks>160</anticipationSoundTicks>
			<ticksToImpactRange>200~250</ticksToImpactRange>
			<angleCurve>
				<points>
					<li>(0,-60)</li>
					<li>(1,-90)</li>
				</points>
			</angleCurve>
			<rotationCurve>
				<points>
					<li>(0,-30)</li>
					<li>(0.5,-5)</li>
					<li>(0.9,-5)</li>
					<li>(0.95,0)</li>
				</points>
			</rotationCurve>
			<zPositionCurve>
				<points>
					<li>(0.95,2.5)</li>
					<li>(1,0)</li>
				</points>
			</zPositionCurve>
			<speedCurve>
				<points>
					<li>(0.6,0.6)</li>
					<li>(0.95,0.1)</li>
				</points>
			</speedCurve>
		</skyfaller>
	</ThingDef>

	<ThingDef ParentName="ShuttleSkyfallerBase" Name="BDMEtest_ShuttleLeavingBase" Abstract="true">
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutFlying</shaderType>
			<color>(1,1,1)</color>
		</graphicData>
		<skyfaller>
			<reversed>true</reversed>
			<anticipationSound>BDM_ThrusterLeaving</anticipationSound>
			<anticipationSoundTicks>0</anticipationSoundTicks>
			<ticksToImpactRange>-40~-15</ticksToImpactRange>
			<moteSpawnTime>0.05</moteSpawnTime>
			<shadow>Things/Skyfaller/SkyfallerShadowRectangle</shadow>
			<motesPerCell>1</motesPerCell>
			<angleCurve>
				<points>
					<li>(0,90)</li>
					<li>(1,70)</li>
				</points>
			</angleCurve>
			<rotationCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.15,10)</li>
					<li>(0.5,-5)</li>
				</points>
			</rotationCurve>
			<zPositionCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.08,2)</li>
				</points>
			</zPositionCurve>
			<speedCurve>
				<points>
					<li>(0,0.2)</li>
					<li>(0.4,0.7)</li>
				</points>
			</speedCurve>
		</skyfaller>
	</ThingDef>

	<WorldObjectDef>
		<defName>BDMEtest_TravelingMedEvacShuttle</defName>
		<label>MedEvac shuttle</label>
		<description>MedEvac shuttle in flight.</description>
		<worldObjectClass>TravelingTransportPods</worldObjectClass>
		<texture>Things/Skyfaller/MedEvacShuttle</texture>
		<useDynamicDrawer>true</useDynamicDrawer>
		<allowCaravanIncidentsWhichGenerateMap>true</allowCaravanIncidentsWhichGenerateMap>
	</WorldObjectDef>

	<ThingDef ParentName="BDMEtest_ShuttleIncomingBase">
		<defName>BDMEtest_MedEvacShuttleIncoming</defName>
		<label>MedEvac shuttle (incoming)</label>
		<thingClass>BillDoorsFramework.Skyfaller_MedEvac</thingClass>
		<size>(5,2)</size>
		<graphicData>
			<texPath>Things/Skyfaller/MedEvacShuttle</texPath>
			<drawSize>7</drawSize>
		</graphicData>
		<skyfaller>
			<shadowSize>(5,3)</shadowSize>
			<zPositionCurve Inherit="false">
				<points>
					<li>(0.8,0.1)</li>
					<li>(1,0)</li>
				</points>
			</zPositionCurve>
			<speedCurve Inherit="false">
				<points>
					<li>(0.6,0.6)</li>
					<li>(0.9,0.05)</li>
				</points>
			</speedCurve>
		</skyfaller>
		<modExtensions>
			<li Class="BillDoorsFramework.SkyfallerMedEvacExtension">
				<leaveShuttleFallerDef>BDMEtest_MedEvacShuttleLeaving</leaveShuttleFallerDef>
				<worldObjectDef>BDMEtest_TravelingMedEvacShuttle</worldObjectDef>
				<returnShuttleFallerDef>BDMEtest_MedEvacShuttleIncomingHome</returnShuttleFallerDef>
			</li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="BDMEtest_ShuttleLeavingBase">
		<defName>BDMEtest_MedEvacShuttleLeaving</defName>
		<label>MedEvac shuttle (leaving)</label>
		<size>(5,2)</size>
		<thingClass>BillDoorsFramework.FlyShipLeaving_MedEvac</thingClass>
		<graphicData>
			<texPath>Things/Skyfaller/MedEvacShuttle</texPath>
			<drawSize>7</drawSize>
		</graphicData>
		<skyfaller>
			<shadowSize>(5,3)</shadowSize>
		</skyfaller>
	</ThingDef>

	<ThingDef ParentName="BDMEtest_ShuttleIncomingBase">
		<defName>BDMEtest_MedEvacShuttleIncomingHome</defName>
		<label>MedEvac shuttle (incoming)</label>
		<size>(5,2)</size>
		<graphicData>
			<texPath>Things/Skyfaller/MedEvacShuttle</texPath>
			<drawSize>7</drawSize>
		</graphicData>
		<skyfaller>
			<shadowSize>(5,3)</shadowSize>
		</skyfaller>
		<modExtensions>
			<li Class="BillDoorsFramework.SkyfallerCustomExtension">
				<returnShuttleFallerDef>BDMEtest_MedEvacShuttleLeavingHome</returnShuttleFallerDef>
			</li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="BDMEtest_ShuttleLeavingBase">
		<defName>BDMEtest_MedEvacShuttleLeavingHome</defName>
		<label>MedEvac shuttle (leaving)</label>
		<size>(5,2)</size>
		<graphicData>
			<texPath>Things/Skyfaller/MedEvacShuttle</texPath>
			<drawSize>7</drawSize>
		</graphicData>
		<skyfaller>
			<shadowSize>(5,3)</shadowSize>
		</skyfaller>
	</ThingDef>
</Defs>