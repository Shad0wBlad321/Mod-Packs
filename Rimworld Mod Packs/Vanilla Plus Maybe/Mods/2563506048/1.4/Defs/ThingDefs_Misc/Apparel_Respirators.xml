<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <!--============ Gasmask ========-->
  
    <!-- P-1944 -->
  
  <ThingDef ParentName="HatMakeableBase">
    <defName>RNApparel_Gasmask_P1944WWI</defName>
    <label>P-1944 gas mask</label>
    <description>A gasmask, also known as a respirator, is a form of headgear designed to cover the face and protect the wearer's respiratory system from being affected by a chemical attack. Depending on style, either the full face is covered, or just the nose and mouth area of the face.\n\nThis version is a commonwealth P1944 gasmask from the great war era.</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
	<techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Things/Apparel/Display/Respirator/Gasmask_P1944</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <drawSize>1.0</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Fabric</li>
	  <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>15</costStuffCount>
	<costList>
      <ComponentSpacer>1</ComponentSpacer>
      <Steel>5</Steel>
    </costList>
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <MaxHitPoints>110</MaxHitPoints>
      <Mass>0.5</Mass>
      <StuffEffectMultiplierArmor>0.20</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.30</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.4</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <ToxicEnvironmentResistance>0.76</ToxicEnvironmentResistance>  
    </equippedStatOffsets>
    <apparel>
	<countsAsClothingForNudity>false</countsAsClothingForNudity>
	<developmentalStageFilter>Child, Adult</developmentalStageFilter>
    <immuneToToxGasExposure>true</immuneToToxGasExposure>
	<bodyPartGroups>
		<li>Mouth</li>
		<li>Eyes</li>
	</bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Headgear/Respirator/Gasmask_P1944</wornGraphicPath>
      <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>
      <layers>
        <li>Overhead</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <tags>
        <li>IndustrialMilitaryBasic</li>
		<li>PassiveCamo_Black_Set</li>
      </tags>	  
	<useWornGraphicMask>true</useWornGraphicMask>
	<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
    </apparel>
  </ThingDef>        
  
