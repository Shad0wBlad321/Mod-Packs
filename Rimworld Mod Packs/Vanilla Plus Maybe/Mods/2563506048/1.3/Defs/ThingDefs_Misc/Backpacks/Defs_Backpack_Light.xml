    </graphicData>
    <apparel>
      <wornGraphicPath>Things/Apparel/Backpack/Maxpedition/Maxpedition_Naotak</wornGraphicPath>
      <wornGraphicData>
        <east>
          <offset>(-0.30,-0.05)</offset> <!-- // (x,y) //--> 
          <thin><offset>(0.05,0)</offset></thin>
          <hulk><offset>(-0.1,0)</offset></hulk>
          <fat><offset>(-0.15,0)</offset></fat>
		  <female><offset>(0.02,0)</offset></female>
        </east>
        <west>
          <offset>(0.30,-0.10)</offset> <!-- // (x,y) //--> 
          <thin><offset>(-0.05,0)</offset></thin>
          <hulk><offset>(0.1,0)</offset></hulk>
          <fat><offset>(0.15,0)</offset></fat>
		  <female><offset>(-0.02,0)</offset></female>		  
        </west>
        <north>
          <offset>(0,-0.1)</offset> <!-- // (x,y) //--> 
          <thin><offset>(0,0)</offset></thin>
          <hulk><offset>(0,-0.05)</offset></hulk>
          <fat><offset>(0,-0.05)</offset></fat>
        </north>		
        <male>  <scale>(0.70,0.70)</scale></male> <!-- // (0.75,0.75) //-->
        <female><scale>(0.70,0.70)</scale></female> <!-- // (0.75,0.75) //-->
        <thin>  <scale>(0.60,0.60)</scale></thin> <!-- // (0.7,0.7) //-->
