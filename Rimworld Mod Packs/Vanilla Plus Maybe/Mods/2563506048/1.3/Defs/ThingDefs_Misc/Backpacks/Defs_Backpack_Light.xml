	<equippedStatOffsets>
	<CarryingCapacity>24</CarryingCapacity>
	</equippedStatOffsets> <!-- Adds backpack capacity with the Backpack.dll-->
	
		<!-- Ingredients -->
	<costStuffCount>55</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
  </ThingDef>  
  
