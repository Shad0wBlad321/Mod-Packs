<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <BodyDef>
    <defName>Mech_Logistic</defName>
    <label>heavy mech with loading claws</label>
    <corePart>
      <def>MechanicalThorax</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>MechanicalNeck</def>
          <coverage>0.10</coverage>
          <customLabel>Mechanical Head Mounting</customLabel>
          <height>Top</height>
          <parts>
            <li>
              <def>MechanicalHead</def>
              <coverage>0.80</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>ArtificialBrain</def>
                  <coverage>0.10</coverage>
                  <depth>Inside</depth>
                </li>
                <li>
                  <def>SightSensor</def>
                  <customLabel>left sight sensor cluster</customLabel>
                  <coverage>0.13</coverage>
                </li>
                <li>
                  <def>SightSensor</def>
                  <customLabel>right sight sensor cluster</customLabel>
                  <coverage>0.13</coverage>
                </li>
                <li>
                  <def>HearingSensor</def>
                  <customLabel>left hearing sensor</customLabel>
                  <coverage>0.10</coverage>
                </li>
                <li>
                  <def>HearingSensor</def>
                  <customLabel>right hearing sensor</customLabel>
                  <coverage>0.10</coverage>
                </li>
                <li>
                  <def>SmellSensor</def>
                  <coverage>0.10</coverage>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>left arm mounting</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>left loading arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>PowerClaw</def>
                  <customLabel>left loading claw</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>LeftPowerClaw</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>right arm mounting</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>right loading arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>PowerClaw</def>
                  <customLabel>right loading claw</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>RightPowerClaw</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left leg mountings</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>left strider array</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right leg mountings</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>right strider array</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>Reactor</def>
          <coverage>0.06</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>FluidReprocessor</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>FluidReprocessor</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
      </parts>
    </corePart>
  </BodyDef>

</Defs>