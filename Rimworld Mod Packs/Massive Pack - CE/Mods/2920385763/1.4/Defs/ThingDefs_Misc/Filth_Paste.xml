<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="BaseFilth">
    <defName>VNPE_Filth_Paste</defName>
    <label>nutrient sludge</label>
    <description>A puddle of nutrient paste.</description>
    <statBases>
      <Beauty>-20</Beauty>
      <Cleanliness>-15</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Spatter</texPath>
      <color>(126, 137, 107, 200)</color>
    </graphicData>
    <filth>
      <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
      <disappearsInDays>35~50</disappearsInDays>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>95</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
    </filth>
  </ThingDef>
</Defs>