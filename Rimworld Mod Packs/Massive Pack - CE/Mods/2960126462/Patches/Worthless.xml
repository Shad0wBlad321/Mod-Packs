<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/TerrainDef[@Name = "FloorBase"]/statBases</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/TerrainDef[@Name = "FloorBase"]</xpath>
			<value>
				<statBases/>
			</value>
		</nomatch>
	</Operation>
