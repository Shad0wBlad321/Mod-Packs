<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>GeneRipper</defName>
		<label>gene ripper</label>
		<description>An advanced gene extractor used to extract a large amount of specific genetic material. Once a pawn is inserted, the Gene Ripper chews through their body, killing them in the process.</description>
		<thingClass>GeneRipper.Building_GeneRipper</thingClass>
		<containedPawnsSelectable>true</containedPawnsSelectable>
		<passability>Impassable</passability>
		<fillPercent>0.4</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>GeneRipper</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,2)</drawSize>
			<shadowData>
				<volume>(3, 2, 1.9)</volume>
			</shadowData>
		</graphicData>
		<size>(3, 2)</size>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<defaultPlacingRot>South</defaultPlacingRot>
		<designationCategory>Biotech</designationCategory>
		<uiOrder>2101</uiOrder>
		<researchPrerequisites>
			<li>GeneProcessor</li>
		</researchPrerequisites>
		<building>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
		</building>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>27000</WorkToBuild>
			<Mass>85</Mass>
			<Flammability>0.5</Flammability>
		</statBases>
		<costList>
			<Steel>200</Steel>
			<Plasteel>100</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
		<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>200</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
	</ThingDef>
</Defs>