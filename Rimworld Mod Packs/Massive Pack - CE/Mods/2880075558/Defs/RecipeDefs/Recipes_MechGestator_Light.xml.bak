<Defs>

  <RecipeDef ParentName="LightMechanoidRecipe">
    <defName>Gastromech</defName>
    <label>gestate gastromech</label>
    <description>Gestate a gastromech mechanoid.</description>
    <researchPrerequisite>StandardMechtech</researchPrerequisite>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreRegular</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Mech_Gastromech>1</Mech_Gastromech>
    </products>
    <descriptionHyperlinks>
      <ThingDef>Mech_Gastromech</ThingDef>
    </descriptionHyperlinks>
	<recipeUsers>
		<li>MechGestator</li>
	</recipeUsers>
  </RecipeDef>

