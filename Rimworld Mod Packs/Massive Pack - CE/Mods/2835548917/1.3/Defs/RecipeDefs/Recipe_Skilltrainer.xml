<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <ThingDef ParentName="UnfinishedBase">
    <defName>USH_UnfinishedSkilltrainer</defName>
    <label>unfinished skilltrainer</label>
    <description>An unfinished skilltrainer.</description>
    <statBases>
      <Flammability>0.5</Flammability>
    </statBases>
    <graphicData>
      <texPath>Things/Item/Misc/UnfinishedSkilltrainer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories Inherit="false" />
  </ThingDef>

   <ThingDef Name="USH_SkilltrainerRecipeBase" Abstract="True">
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>42000</workAmount>
	<skillRequirements>
      <Intellectual>12</Intellectual>
	  <Crafting>5</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
	<unfinishedThingDef>USH_UnfinishedSkilltrainer</unfinishedThingDef>
	<ingredients>
	  <li>
        <filter>
          <thingDefs>
            <li>Silver</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>MedicineUltratech</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>USH_GlitterChip</li>
          </thingDefs>
        </filter>
        <count>8</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
	    <li>Silver</li>
        <li>ComponentSpacer</li>
        <li>MedicineUltratech</li>
		<li>USH_GlitterChip</li>
      </thingDefs>
    </fixedIngredientFilter>
  </ThingDef>


