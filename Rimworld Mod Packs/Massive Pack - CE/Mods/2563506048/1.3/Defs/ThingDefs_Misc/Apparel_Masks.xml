<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--============ Balaclavas ========-->
  
    <!-- Balaclava -->
  
  <ThingDef ParentName="HatMakeableBase">
    <defName>RNApparel_Balaclava_EXF</defName>
    <label>balaclava (EXF)</label>
    <description>A balaclava, also known as a balaclava helmet or Bally (UK slang) or ski mask (US slang), is a form of cloth headgear designed to expose only part of the face, usually the eyes and mouth. Depending on style and how it is worn, only the eyes, mouth and nose, or just the front of the face are unprotected.\n\nThis version is the Team Wendy EXF line of balaclavas.</description>
    <techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Things/Apparel/Display/Headgear/Balaclava_EXF</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
	  <drawSize>0.70</drawSize>
    </graphicData>
    <stuffCategories>
      <li>Fabric</li>
	  <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>15</costStuffCount>
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <MaxHitPoints>110</MaxHitPoints>
      <Mass>0.05</Mass>
      <StuffEffectMultiplierArmor>0.10</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.30</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.04</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.2</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <PainShockThreshold>0.05</PainShockThreshold>
    </equippedStatOffsets>
    <apparel>
	<countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <!--<li>Mouth</li>
        <li>Teeth</li>-->
        <li>Neck</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Headgear/Balaclava/Balaclava_EXF</wornGraphicPath>
      <!--<hatRenderedFrontOfFace>true</hatRenderedFrontOfFace>-->
      <layers>
        <li>Overhead</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <tags>
       <!--<li>IndustrialMilitaryAdvanced</li>-->
		<li>PassiveCamo_Black_Set</li>
      </tags>	  
	<useWornGraphicMask>true</useWornGraphicMask>
	<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
    </apparel>
	<generateCommonality>0.10</generateCommonality>
	<generateAllowChance>0.10</generateAllowChance>  
  </ThingDef>    
  
