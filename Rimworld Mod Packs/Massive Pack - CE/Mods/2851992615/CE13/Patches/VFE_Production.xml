<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Production</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ThingDef[@Name="M41RocketUNSCBase" or @Name="40x46mmGrenadeUNSCBase"]/tradeTags</xpath>
                    <value>
                        <li>CE_AutoEnableCrafting_VFE_TableMachiningLarge</li>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>