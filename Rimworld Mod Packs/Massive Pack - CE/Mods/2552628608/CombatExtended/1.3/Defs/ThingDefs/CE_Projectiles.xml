  <!-- ================== 6x18mm ================== -->
  <ThingDef Name="BaseEccentric6x18mmChargedBullet" ParentName="Base6x18mmChargedBullet" Abstract="True">
    <thingClass>EccentricProjectiles.ProjectileCE_WithEffects</thingClass>
    <graphicData>
      <texPath>Things/Eccentric/Projectile/ChargeProjectile_Small</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
    </graphicData>
    <modExtensions>
      <li Class="EccentricProjectiles.ModExtension_ProjectileEffects">
        <effects>
          <li>Eccentric_ChargeMuzzleFlashSmall</li>
        </effects>
      </li>
    </modExtensions>
  </ThingDef>
  <!-- normal charged bullets -->
  <ThingDef ParentName="BaseEccentric6x18mmChargedBullet">
    <defName>Bullet_Eccentric6x18mmCharged</defName>
    <label>6x18mm Charged bullet</label>
    <projectile Class="CombatExtended.ProjectilePropertiesCE">
      <damageAmountBase>10</damageAmountBase>
      <secondaryDamage>
        <li>
          <def>Bomb_Secondary</def>
          <amount>3</amount>
        </li>
      </secondaryDamage>
      <armorPenetrationSharp>12</armorPenetrationSharp>
      <armorPenetrationBlunt>14.4</armorPenetrationBlunt>
    </projectile>
  </ThingDef>
