<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 <ThingDef ParentName="MealBase">
    <defName>FTC_MealStew</defName>
    <label>Stew</label>
    <description>A combination of solid food ingredients that have been cooked in liquid.</description>
    <graphicData>
      <texPath>Things/Stew</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>2</MarketValue>
      <Nutrition>0.85</Nutrition>
      <FoodPoisonChanceFixedHuman>0</FoodPoisonChanceFixedHuman>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>0.25</daysToRotStart>  <!-- short rot time, you want to make them as needed not store them -->
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

</Defs>