            <MentalBreakThreshold>0.2</MentalBreakThreshold>
        </statOffsets>
        <statFactors>
            <RestFallRateFactor>0.1</RestFallRateFactor>
        </statFactors>
        <suppressedTraits>
            <li>
                <def>Delicate</def>
            </li>
            <li>
                <def>Wimp</def>
            </li>
            <li>
                <def>Industriousness</def>
