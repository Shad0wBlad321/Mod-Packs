        <biostatCpx>3</biostatCpx>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <backgroundPathXenogenes>UI/Icons/Genes/GeneBackground_Undivided</backgroundPathXenogenes>
                <hediffsToBodyParts>
                    <li>
                        <hediff>BEWH_DaemonicHornAttack</hediff>
                        <bodyparts>
                            <li>Head</li>
                        </bodyparts>
                    </li>
                </hediffsToBodyParts>
            </li>
        </modExtensions>
    </GeneDef>

