<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Space Marine Base -->

    <PawnKindDef>
        <defName>BEWH_ImperiumTrader</defName>
        <race>Human</race>
        <defaultFactionType>BEWH_ImperiumFaction</defaultFactionType>
        <initialWillRange>300~400</initialWillRange>
        <initialResistanceRange>300~400</initialResistanceRange>
        <acceptArrestChanceFactor>0</acceptArrestChanceFactor>
        <invNutrition>2.55</invNutrition>
        <backstoryCryptosleepCommonality>1.0</backstoryCryptosleepCommonality>
        <minGenerationAge>19</minGenerationAge>
        <maxGenerationAge>21</maxGenerationAge>
        <combatPower>40</combatPower>
        <trader>true</trader>
        <isFighter>false</isFighter>
        <itemQuality>Good</itemQuality>
        <ignoreIdeoApparelColors>true</ignoreIdeoApparelColors>
        <useFactionXenotypes>false</useFactionXenotypes>
        <gearHealthRange>
            <min>1</min>
            <max>1</max>
        </gearHealthRange>
        <weaponTags>
            <li>SpacerGun</li>
        </weaponTags>
        <weaponMoney>1100~3500</weaponMoney>
        <apparelMoney>1250~2500</apparelMoney>
        <apparelTags>
            <li>IndustrialBasic</li>
            <li>IndustrialAdvanced</li>
            <li>IndustrialMilitaryBasic</li>
            <li>IndustrialMilitaryAdvanced</li>
        </apparelTags>
        <apparelDisallowTags>
            <li>Western</li>
        </apparelDisallowTags>
        <apparelColor>(250,250,250)</apparelColor>
        <apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
        <specificApparelRequirements Inherit="False">
            <li>
                <bodyPartGroup>Torso</bodyPartGroup>
                <apparelLayer>OnSkin</apparelLayer>
                <stuff>Cloth</stuff>
            </li>
            <li>
                <bodyPartGroup>Legs</bodyPartGroup>
                <apparelLayer>OnSkin</apparelLayer>
                <stuff>Cloth</stuff>
            </li>
        </specificApparelRequirements>
        <inventoryOptions>
            <subOptionsChooseOne>
                <li>
                    <thingDef>MealSurvivalPack</thingDef>
                    <countRange>
                        <min>4</min>
                        <max>5</max>
                    </countRange>
                </li>
                <li>
                    <thingDef>MedicineUltratech</thingDef>
                    <countRange>
                        <min>2</min>
                        <max>5</max>
                    </countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
        <xenotypeSet>
            <xenotypeChances>
                <Baseliner>1</Baseliner>
            </xenotypeChances>
        </xenotypeSet>
    </PawnKindDef>

</Defs>